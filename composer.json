{
    "name": "eresponseservices/mastercard-sdk-core-php",
    "description": "This is the core or base package of the MasterCard Api, all different flavours of Api provided by MasterCard will reuse this package",
    "version": "1.5.0",
    "repositories": [{
        "type": "composer",
        "url": "https://satis.chargebacks911.com"
    }],
    "license": "proprietary",
    "authors": [
        {
            "name": "MasterCard Worldwide",
            "email": "APISupport@mastercard.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.1 || ^7.0",
        "monolog/monolog": "~1.22 || ~2.0 || ~3.0"
    },
    "require-dev": {
	    "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-0": {
            "MasterCard": ""
        }
    },
    "autoload-dev": {
        "psr-0": {
            "MasterCard": ["MasterCard/", "Test/"]
        }
    }
}
